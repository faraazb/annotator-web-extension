{
  "name": "annotator-web-extension",
  "version": "1.0.1",
  "scripts": {
    "start": "parcel watch src/manifest.json --host localhost --config @parcel/config-webextension",
    "build": "parcel build src/manifest.json --config @parcel/config-webextension --dist-dir build --no-content-hash"
  },
  "devDependencies": {
    "@parcel/config-webextension": "^2.8.3",
    "@parcel/transformer-inline-string": "^2.8.3",
    "chrome-types": "^0.1.181",
    "parcel": "^2.8.3"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.7",
    "@preact/signals": "^1.1.3",
    "downshift": "^7.6.0",
    "history": "^5.3.0",
    "image-blob-reduce": "^4.1.0",
    "konva": "^8.4.3",
    "preact": "^10.13.1",
    "preact-router": "^4.1.0",
    "react-draggable": "^4.4.5",
    "react-hot-toast": "^2.4.0",
    "react-popper": "^2.3.0",
    "teaful": "^0.11.1"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom": "preact/compat"
  },
  "@parcel/bundler-default": {
    "minBundles": 10000000,
    "minBundleSize": 3000,
    "maxParallelRequests": 20
  }
}
